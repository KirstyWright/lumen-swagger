{
    "name": "benjamincrozat/lumen-swagger",
    "description": "Generate API documentation with swag.",
    "keywords": [
        "api",
        "lumen",
        "restful",
        "swagger"
    ],
    "license": "WTFPL",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "illuminate/console": "~5.5|~5.6",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BC\\LumenSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BC\\LumenSwagger\\ServiceProvider"
            ]
        }
    }
}
